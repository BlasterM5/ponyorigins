{
    "name": "Friend of All",
    "description": "Press to tame any tameable creature.\n\nAll mobs will ignore you.\n\nYOU CANNOT HARM ANOTHER LIVING CREATURE",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Press [",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] to tame a creature"
            ],
            "sprite": "textures/particle/heart.png"
        },
        {
            "type": "origins:tooltip",
            "text": "ALL ATTACKS DO NO DAMAGE",
            "sprite": "textures/block/rose_bush_top.png"
        },
        {
            "type": "origins:tooltip",
            "text": "50% Damage Resistance",
            "sprite": "textures/particle/bubble.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Some blocks can't be placed, broken, or used near other creatures",
            "sprite": "textures/block/tnt_side.png"
