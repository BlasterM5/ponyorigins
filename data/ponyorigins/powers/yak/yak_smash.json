{
    "name": "YAK SMASH",
    "description": "Hold your primary power with an empty hoof to SMASH your opponent!\n\nHold your secondary power with empty hooves to STOMP the ground, launching nearby creatures skyward!",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Hold [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to SMASH"
            ],
            "sprite": "textures/block/tnt_side.png"
        },
        {
            "type": "origins:tooltip",
            "text": [
                "Hold [",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] to STOMP"
            ],
            "sprite": "textures/block/tnt_side.png"
        }
    ],
    "type": "origins:multiple",
    "buffer_smash": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 1,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer_smash",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "buffer_stomp": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 0,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer_stomp",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "cd_smash": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "cd_stomp": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "cd_recharge_smash": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd_smash",
            "comparison": "==",
            "compare_to": 1
        },
        "type": "origins:action_over_time",
        "interval": 10,
        "rising_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd_smash",
            "change": 0,
            "operation": "set"
        }
    },
    "cd_recharge_stomp": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd_stomp",
            "comparison": "==",
            "compare_to": 1
        },
        "type": "origins:action_over_time",
        "interval": 10,
        "rising_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd_stomp",
            "change": 0,
            "operation": "set"
        }
    },
    "tick": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer_smash",
                                "comparison": "==",
                                "compare_to": 10
                            },
                            {
                                "type": "origins:resource",
                                "resource": "ponyorigins:common/00_key_primary_active",
                                "comparison": ">",
                                "compare_to": 0
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:raycast",
                        "distance": 3,
                        "block": true,
                        "entity": true,
                        "shape_type": "outline",
                        "fluid_handling": "none",
                        "bientity_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:add_velocity",
                                    "x": 0,
                                    "y": 0.25,
                                    "z": 2,
                                    "client": false,
                                    "server": true
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:play_sound",
                                                "sound": "entity.generic.explode",
                                                "volume": 1,
                                                "pitch": 1
                                            },
                                            {
                                                "type": "origins:spawn_particles",
                                                "particle": "flash",
                                                "count": 1,
                                                "speed": 1,
                                                "force": true,
                                                "spread": {
                                                    "x": 0.2,
                                                    "y": 0.2,
                                                    "z": 0.2
                                                }
                                            },
                                            {
                                                "type": "origins:damage",
                                                "amount": 8,
                                                "damage_type": "player_attack"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        "block_action": {
                            "type": "origins:if_else",
                            "condition": {
                                "inverted": true,
                                "type": "origins:in_tag",
                                "tag": "origins:unphasable"
                            },
                            "if_action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "apugli:destroy",
                                        "drop_block": true
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "playsound entity.generic.explode block @a ~ ~ ~"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "particle flash ~ ~ ~ 0.2 0.2 0.2 1 1 force"
                                    }
                                ]
                            }
                        },
                        "hit_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer_smash",
                                    "change": 0,
                                    "operation": "set"
                                },
                                {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_cd_smash",
                                    "change": 1,
                                    "operation": "set"
                                },
                                {
                                    "type": "origins:swing_hand",
                                    "hand": "MAIN_HAND"
                                },
                                {
                                    "type": "origins:exhaust",
                                    "amount": 2
                                }
                            ]
                        }
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer_stomp",
                                "comparison": "==",
                                "compare_to": 10
                            },
                            {
                                "type": "origins:resource",
                                "resource": "ponyorigins:common/00_key_secondary_active",
                                "comparison": ">",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:on_block",
                                "block_condition": {
                                    "type": "origins:movement_blocking"
                                }
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:swing_hand",
                                "hand": "MAIN_HAND"
                            },
                            {
                                "type": "origins:swing_hand",
                                "hand": "OFF_HAND"
                            },
                            {
                                "type": "origins:spawn_particles",
                                "particle": "flash",
                                "count": 1,
                                "speed": 1,
                                "force": true,
                                "spread": {
                                    "x": 0.2,
                                    "y": 0.2,
                                    "z": 0.2
                                }
                            },
                            {
                                "type": "origins:play_sound",
                                "sound": "entity.generic.explode",
                                "volume": 1,
                                "pitch": 1
                            },
                            {
                                "type": "origins:exhaust",
                                "amount": 2
                            },
                            {
                                "type": "origins:area_of_effect",
                                "radius": 8,
                                "bientity_action": {
                                    "type": "origins:add_velocity",
                                    "x": 0,
                                    "y": 1,
                                    "z": 0,
                                    "client": false,
                                    "server": true
                                }
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_buffer_stomp",
                                "change": 0,
                                "operation": "set"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_cd_stomp",
                                "change": 1,
                                "operation": "set"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:or",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer_smash",
                                "comparison": ">",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer_stomp",
                                "comparison": ">",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:resource",
                                "resource": "ponyorigins:common/00_key_primary_active",
                                "comparison": ">",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:resource",
                                "resource": "ponyorigins:common/00_key_secondary_active",
                                "comparison": ">",
                                "compare_to": 0
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:if_else",
                                "condition": {
                                    "type": "origins:and",
                                    "conditions": [
                                        {
                                            "type": "origins:resource",
                                            "resource": "ponyorigins:common/00_key_primary_active",
                                            "comparison": ">",
                                            "compare_to": 0
                                        },
                                        {
                                            "type": "origins:resource",
                                            "resource": "ponyorigins:common/00_empty_mainhand",
                                            "comparison": "==",
                                            "compare_to": 1
                                        },
                                        {
                                            "type": "origins:food_level",
                                            "comparison": ">",
                                            "compare_to": 4
                                        }
                                    ]
                                },
                                "if_action": {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer_smash",
                                    "change": 1,
                                    "operation": "add"
                                },
                                "else_action": {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer_smash",
                                    "change": -1,
                                    "operation": "add"
                                }
                            },
                            {
                                "type": "origins:if_else",
                                "condition": {
                                    "type": "origins:and",
                                    "conditions": [
                                        {
                                            "type": "origins:resource",
                                            "resource": "ponyorigins:common/00_key_secondary_active",
                                            "comparison": ">",
                                            "compare_to": 0
                                        },
                                        {
                                            "type": "origins:resource",
                                            "resource": "ponyorigins:common/00_empty_mainhand",
                                            "comparison": "==",
                                            "compare_to": 1
                                        },
                                        {
                                            "type": "origins:resource",
                                            "resource": "ponyorigins:common/00_empty_offhand",
                                            "comparison": "==",
                                            "compare_to": 1
                                        },
                                        {
                                            "type": "origins:food_level",
                                            "comparison": ">",
                                            "compare_to": 4
                                        }
                                    ]
                                },
                                "if_action": {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer_stomp",
                                    "change": 1,
                                    "operation": "add"
                                },
                                "else_action": {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer_stomp",
                                    "change": -1,
                                    "operation": "add"
                                }
                            }
                        ]
                    }
                }
            ]
        }
    }
}