                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 50
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 1,
                        "operation": "add"
                    }
                ]
            }
        },
        "falling_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 10
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "block.fire.extinguish",
                        "volume": 0.1,
                        "pitch": 1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd",
                        "change": 1,
                        "operation": "set"
                    }
                ]
            }
        }
    },
    "trigger": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:unicorn/firecracker"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_primary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/firecracker"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_secondary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:simple"
    },
    "spark": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": "<",
                "compare_to": 10
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:instant_effect ~ ~ ~ 0 0.1 0 1 0 force"
            },
            "else_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:electric_spark ~ ~ ~ 0.025 0.025 0.025 1 4 force"
            }
        }
    }
}