{
    "name": "Flamethrower",
    "description": "Hold your [primary/secondary] power to release your energy directly as a burst of flame!",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Primary Talent: Hold [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to burn everything"
            ],
            "sprite": "minecraft:textures/particle/flame.png"
        },
        {
            "type": "origins:tooltip",
            "text": [
                "Secondary Talent: Hold [",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] to burn everything"
            ],
            "sprite": "minecraft:textures/particle/flame.png"
        }
    ],
    "type": "origins:multiple",
    "flamethrower": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_charged",
                    "comparison": "==",
                    "compare_to": 1
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 2,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "inverted": true,
                        "type": "origins:submerged_in",
                        "fluid": "minecraft:water"
                    },
                    "if_action": {
                        "type": "origins:raycast",
                        "distance": 10,
                        "block": true,
                        "entity": true,
                        "shape_type": "collider",
                        "fluid_handling": "any",
                        "bientity_action": {
                            "type": "origins:target_action",
                            "action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:damage",
                                        "amount": 2,
                                        "damage_type": "on_fire"
                                    },
                                    {
                                        "type": "origins:set_on_fire",
                                        "duration": 4
                                    },
                                    {
                                        "type": "origins:block_action_at",
                                        "block_action": {
                                            "type": "origins:add_block",
                                            "block": "minecraft:fire"
                                        }
                                    }
                                ]
                            }
                        },
                        "block_action": {
                            "type": "origins:add_block",
                            "block": "minecraft:fire"
                        },
                        "before_action": {
                            "type": "origins:play_sound",
                            "sound": "block.fire.ambient",
                            "volume": 0.5,
                            "pitch": 0.5
                        },
                        "command_along_ray": "particle minecraft:flame ^ ^0.5 ^ 0 0 0 1 0 force",
                        "command_step": 1
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:raycast",
                                "distance": 3,
                                "block": false,
                                "entity": false,
                                "shape_type": "visual",
                                "fluid_handling": "none",
                                "command_along_ray": "particle minecraft:bubble_column_up ^ ^0.5 ^ 0 0 0 1 8 force",
                                "command_step": 1
                            },
                            {
                                "type": "origins:play_sound",
                                "sound": "minecraft:block.bubble_column.upwards_inside",
                                "volume": 1,
                                "pitch": 1
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:power_active",
                                "power": "*:*_trigger"
                            },
                            {
                                "type": "origins:resource",
                                "resource": "ponyorigins:common/energy_level",
                                "comparison": ">",
                                "compare_to": 0
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -1,
                        "operation": "add"
                    },
                    "else_action": {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": -1,
                        "operation": "add"
                    }
                }
            ]
        }
    },
    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 7,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": ">",
                "compare_to": 0
            }
        },
        "min_action": {
            "type": "origins:change_resource",
            "resource": "*:*_charged",
            "change": 0,
            "operation": "set"
        },
        "max_action": {
            "type": "origins:change_resource",
            "resource": "*:*_charged",
            "change": 1,
            "operation": "add"
        }
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": -1,
            "operation": "add"
        }
    },
    "buffer_return": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "inverted": true,
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 5
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_charged",
                        "comparison": "==",
                        "compare_to": 0
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": 1,
                        "operation": "add"
                    }
                ]
            },
            "else_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">=",
                    "compare_to": 5
                },
                "if_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_charged",
                    "change": 1,
                    "operation": "set"
                }
            }
        }
    },
    "charged": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "charge": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 10
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 1,
                        "operation": "add"
                    }
                ]
            },
            "else_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": "==",
                    "compare_to": 10
                },
                "if_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_charged",
                    "change": 1,
                    "operation": "set"
                }
            }
        },
        "falling_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_charged",
                        "comparison": "==",
                        "compare_to": 0
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "block.fire.extinguish",
                        "volume": 0.1,
                        "pitch": 1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd",
                        "change": 1,
                        "operation": "set"
                    }
                ]
            }
        }
    },
    "trigger": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:unicorn/flamethrower"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_primary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/flamethrower"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_secondary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:simple"
    },
    "spark": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "*:*_charged",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 5
                    }
                ]
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:instant_effect ~ ~ ~ 0 0.1 0 1 0 force"
            },
            "else_action": {
                "type": "origins:if_else",
                "condition": {
                    "inverted": true,
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                },
                "if_action": {
                    "type": "origins:execute_command",
                    "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:flame ~ ~ ~ 0 0.1 0 1 0 force"
                },
                "else_action": {
                    "type": "origins:execute_command",
                    "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:bubble_column_up ~ ~ ~ 0 0.1 0 1 0 force"
                }
            }
        }
    }
}