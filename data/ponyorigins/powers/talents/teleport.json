                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer",
                                    "change": -10,
                                    "operation": "add"
                                },
                                {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_cd",
                                    "change": 2,
                                    "operation": "set"
                                }
                            ]
                        },
                        "command_at_hit": "function ponyorigins:teleport"
                    }
