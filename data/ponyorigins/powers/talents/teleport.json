    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 100,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 6,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 2,
        "start_value": 0
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": -1,
            "operation": "add"
        }
    },
    "charge": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "ponyorigins:common/energy_level",
                "comparison": ">=",
                "compare_to": 5
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -5,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 5,
                        "operation": "add"
                    }
                ]
            }
        },
        "falling_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": ">",
                        "compare_to": 0
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "block.fire.extinguish",
                        "volume": 0.1,
                        "pitch": 1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd",
                        "change": 2,
                        "operation": "set"
                    }
                ]
            }
        }
    },
    "trigger": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/teleport",
                            "layer": "ponyorigins:primary_talent"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_primary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/teleport",
                            "layer": "ponyorigins:secondary_talent"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_secondary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:simple"
    },
    "tick": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_buffer",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "inverted": true,
                        "type": "origins:power_active",
                        "power": "*:*_trigger"
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_buffer",
                                "change": -1,
                                "operation": "add"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "ponyorigins:common/energy_level",
                                "change": 1,
                                "operation": "add"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle instant_effect ~ ~ ~ 0 0.1 0 1 0 force"
                    }
                }
            ]
        }
    }
}