{
    "name": "Kinetic Burst",
    "description": "Hold your [primary/secondary] power to focus your telekinesis in a single, explosive shove in the direction you are looking!",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Primary Talent: Hold [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to charge, release to fire"
            ],
            "sprite": "minecraft:textures/block/tnt_side.png"
        },
        {
            "type": "origins:tooltip",
            "text": [
                "Secondary Talent: Hold [",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] to charge and fire"
            ],
            "sprite": "minecraft:textures/block/tnt_side.png"
        }
    ],
    "type": "origins:multiple",
    "laser": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_charged",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": "==",
                    "compare_to": 20
                },
                {
                    "inverted": true,
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:raycast",
                    "distance": 20,
                    "block": true,
                    "entity": true,
                    "shape_type": "collider",
                    "fluid_handling": "any",
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:explode",
                                    "power": 1,
                                    "destruction_type": "none",
                                    "damage_self": true
                                }
                            },
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:spawn_particles",
                                    "particle": "minecraft:flash",
                                    "count": 1,
                                    "speed": 1,
                                    "spread": {
                                        "x": 0,
                                        "y": 0,
                                        "z": 0
                                    }
                                }
                            },
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:play_sound",
                                    "sound": "entity.generic.explode",
                                    "volume": 1,
                                    "pitch": 1
                                }
                            }
                        ]
                    },
                    "block_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:explode",
                                "power": 1,
                                "destruction_type": "none",
                                "damage_self": true
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "particle minecraft:flash ~ ~ ~ 0 0 0 1 1 force"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound entity.generic.explode master @a"
                            }
                        ]
                    },
                    "command_along_ray": "particle minecraft:instant_effect ^ ^0.5 ^ 0.1 0.1 0.1 10 8 force",
                    "command_step": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -20,
                    "operation": "add"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_cd",
                    "change": 1,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_charged",
                    "change": 0,
                    "operation": "set"
                }
            ]
        }
    },
    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 20,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 3,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": -1,
            "operation": "add"
        }
    },
    "buffer_return": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "inverted": true,
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": "<",
                "compare_to": 20
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": 1,
                        "operation": "add"
                    }
                ]
            },
            "else_action": {
                "type": "origins:change_resource",
                "resource": "*:*_charged",
                "change": 1,
                "operation": "set"
            }
        }
    },
    "charge": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 20
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 1,
                        "operation": "add"
                    }
                ]
            }
        },
        "falling_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<",
                        "compare_to": 20
                    },
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": "==",
                        "compare_to": 0
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd",
                        "change": 1,
                        "operation": "set"
                    },
                    {
                        "type": "origins:play_sound",
                        "sound": "block.fire.extinguish",
                        "volume": 0.1,
                        "pitch": 1
                    }
                ]
            }
        }
    },
    "charged": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "trigger": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:unicorn/laser"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_primary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/laser"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_secondary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:simple"
    },
    "spark": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": "<",
                "compare_to": 20
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:instant_effect ~ ~ ~ 0 0.1 0 1 0 force"
            },
            "else_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:instant_effect ~ ~ ~ 0.025 0.025 0.025 1 4 force"
            }
        }
    }
}