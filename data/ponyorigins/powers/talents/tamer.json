{
    "name": "Friend of the Animals",
    "description": "Press your [primary/secondary] power key while looking at a tameable creature to tame them.",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Press [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "/",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] on a tameable creature"
            ],
            "sprite": "minecraft:textures/particle/heart.png"
        }
    ],
    "type": "origins:multiple",
    "tame_primary": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:origin",
                    "origin": "ponyorigins:unicorn/tamer"
                },
                {
                    "type": "origins:resource",
                    "resource": "ponyorigins:common/energy_level",
                    "comparison": ">=",
                    "compare_to": 25
                }
            ]
        },
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:raycast",
            "distance": 10,
            "block": false,
            "entity": true,
            "shape_type": "outline",
            "fluid_handling": "any",
            "bientity_action": {
                "type": "origins:if_else_list",
                "actions": [
                    {
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "type": "origins:in_tag",
                                        "tag": "ponyorigins:tameable"
                                    }
                                },
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "inverted": true,
                                        "type": "origins:tamed"
                                    }
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:tame"
                                },
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "ponyorigins:common/energy_level",
                                        "change": -25,
                                        "operation": "add"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:spawn_particles",
                                        "particle": "heart",
                                        "count": 16,
                                        "speed": 1,
                                        "force": true,
                                        "spread": {
                                            "x": 1,
                                            "y": 1,
                                            "z": 1
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "type": "origins:in_tag",
                                        "tag": "ponyorigins:tameable_rides"
                                    }
                                },
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "inverted": true,
                                        "type": "origins:nbt",
                                        "nbt": "{Tame:1b}"
                                    }
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "data modify entity @s Tame set value 1b"
                                    }
                                },
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "ponyorigins:common/energy_level",
                                        "change": -25,
                                        "operation": "add"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:spawn_particles",
                                        "particle": "heart",
                                        "count": 16,
                                        "speed": 1,
                                        "force": true,
                                        "spread": {
                                            "x": 1,
                                            "y": 1,
                                            "z": 1
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "type": "origins:in_tag",
                                        "tag": "ponyorigins:tameable_trust"
                                    }
                                },
                                {
                                    "type": "origins:target_condition",
                                    "condition": {
                                        "type": "origins:command",
                                        "command": "tag @s list",
                                        "comparison": "==",
                                        "compare_to": 0
                                    }
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "data modify entity @s Trusted append from entity @p UUID"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "tag @s add po.tamed"
                                    }
                                },
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "ponyorigins:common/energy_level",
                                        "change": -25,
                                        "operation": "add"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:spawn_particles",
                                        "particle": "heart",
                                        "count": 16,
                                        "speed": 1,
                                        "force": true,
                                        "spread": {
                                            "x": 1,
                                            "y": 1,
                                            "z": 1
                                        }
                                    }
                                }
                            ]
                        }
                    }
                ]
            },
            "command_along_ray": "particle heart ^ ^-0.25 ^ 0 0 0 0.1 1 force",
            "command_along_ray_only_on_hit": true
        },
        "cooldown": 10,
        "key": {
            "key": "key.origins.primary_active"
        }
    },
