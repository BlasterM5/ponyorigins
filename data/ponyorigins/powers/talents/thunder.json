{
    "name": "Thunder",
    "description": "Hold your [primary/secondary] power to attract lightning to your opponent! Works best in rain.",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Primary Talent: Hold [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to attract lightning"
            ],
            "sprite": "minecraft:textures/block/lightning_rod.png"
        },
        {
            "type": "origins:tooltip",
            "text": [
                "Secondary Talent: Hold [",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "] to attract lightning"
            ],
            "sprite": "minecraft:textures/block/lightning_rod.png"
        }
    ],
    "type": "origins:multiple",
    "thunder": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_weather",
                    "comparison": ">=",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:or",
                "conditions": [
                    {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_weather",
                                "comparison": "==",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer",
                                "comparison": "==",
                                "compare_to": 80
                            }
                        ]
                    },
                    {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_weather",
                                "comparison": "==",
                                "compare_to": 1
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer",
                                "comparison": ">=",
                                "compare_to": 40
                            }
                        ]
                    },
                    {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_weather",
                                "comparison": "==",
                                "compare_to": 2
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_buffer",
                                "comparison": ">=",
                                "compare_to": 20
                            }
                        ]
                    }
                ]
            },
            "if_action": {
                "type": "origins:raycast",
                "distance": 40,
                "block": true,
                "entity": true,
                "shape_type": "collider",
                "fluid_handling": "any",
                "bientity_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:target_action",
                            "action": {
                                "type": "origins:execute_command",
                                "command": "summon lightning_bolt ~ ~ ~"
                            }
                        },
                        {
                            "type": "origins:target_action",
                            "action": {
                                "type": "origins:execute_command",
                                "command": "particle electric_spark ~ ~0.5 ~ 0.1 0.1 0.1 1 16 force"
                            }
                        }
                    ]
                },
                "block_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "summon lightning_bolt ~ ~ ~"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "particle electric_spark ~ ~0.5 ~ 0.1 0.1 0.1 1 16 force"
                        }
                    ]
                },
                "hit_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:if_else_list",
                            "actions": [
                                {
                                    "condition": {
                                        "type": "origins:resource",
                                        "resource": "*:*_weather",
                                        "comparison": "==",
                                        "compare_to": 2
                                    },
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "*:*_buffer",
                                        "change": -20,
                                        "operation": "add"
                                    }
                                },
                                {
                                    "condition": {
                                        "type": "origins:resource",
                                        "resource": "*:*_weather",
                                        "comparison": "==",
                                        "compare_to": 1
                                    },
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "*:*_buffer",
                                        "change": -40,
                                        "operation": "add"
                                    }
                                },
                                {
                                    "condition": {
                                        "type": "origins:resource",
                                        "resource": "*:*_weather",
                                        "comparison": "==",
                                        "compare_to": 0
                                    },
                                    "action": {
                                        "type": "origins:change_resource",
                                        "resource": "*:*_buffer",
                                        "change": -80,
                                        "operation": "add"
                                    }
                                }
                            ]
                        },
                        {
                            "type": "origins:change_resource",
                            "resource": "*:*_cd",
                            "change": 1,
                            "operation": "set"
                        }
                    ]
                }
            }
        }
    },
    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 80,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_buffer",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "weather": {
        "type": "origins:resource",
        "min": -1,
        "max": 2,
        "start_value": 0
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": -1,
            "operation": "add"
        }
    },
    "buffer_return": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "inverted": true,
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -1,
                    "operation": "add"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "ponyorigins:common/energy_level",
                    "change": 1,
                    "operation": "add"
                }
            ]
        }
    },
    "charge": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_trigger"
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "ponyorigins:common/energy_level",
                "comparison": ">",
                "compare_to": 0
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "*:*_buffer",
                            "comparison": "<",
                            "compare_to": 80
                        },
                        "if_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:change_resource",
                                    "resource": "ponyorigins:common/energy_level",
                                    "change": -1,
                                    "operation": "add"
                                },
                                {
                                    "type": "origins:change_resource",
                                    "resource": "*:*_buffer",
                                    "change": 1,
                                    "operation": "add"
                                }
                            ]
                        }
                    },
                    {
                        "type": "origins:raycast",
                        "distance": 40,
                        "block": true,
                        "entity": true,
                        "shape_type": "collider",
                        "fluid_handling": "any",
                        "command_at_hit": "particle electric_spark ~ ~0.5 ~ 0.1 0.1 0.1 1 8 force"
                    }
                ]
            }
        },
        "falling_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:resource",
                        "resource": "*:*_cd",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "ponyorigins:common/energy_level",
                        "comparison": "==",
                        "compare_to": 0
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:play_sound",
                        "sound": "minecraft:block.fire.extinguish",
                        "volume": 0.1,
                        "pitch": 1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd",
                        "change": 1,
                        "operation": "set"
                    }
                ]
            }
        }
    },
    "trigger": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:unicorn/thunder"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_primary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:origin",
                            "origin": "ponyorigins:talents/thunder"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "ponyorigins:common/00_key_secondary_active",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:simple"
    },
    "spark": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_buffer",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute anchored eyes positioned ^ ^0.5 ^0.5 run particle minecraft:instant_effect ~ ~ ~ 0 0.1 0 1 0 force"
        }
    },
    "weather_check": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_buffer",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:or",
                "conditions": [
                    {
                        "inverted": true,
                        "type": "origins:exposed_to_sky"
                    },
                    {
                        "type": "origins:biome",
                        "biome": "ad_astra:orbit"
                    }
                ]
            },
            "if_action": {
                "type": "origins:change_resource",
                "resource": "*:*_weather",
                "change": -1,
                "operation": "set"
            },
            "else_action": {
                "type": "origins:if_else_list",
                "actions": [
                    {
                        "condition": {
                            "type": "origins:or",
                            "conditions": [
                                {
                                    "type": "origins:predicate",
                                    "predicate": "ponyorigins:weather/is_dry_thunder"
                                },
                                {
                                    "type": "origins:predicate",
                                    "predicate": "ponyorigins:weather/is_lightning"
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:change_resource",
                            "resource": "*:*_weather",
                            "change": 2,
                            "operation": "set"
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:or",
                            "conditions": [
                                {
                                    "type": "origins:predicate",
                                    "predicate": "ponyorigins:weather/is_rain"
                                },
                                {
                                    "type": "origins:biome",
                                    "condition": {
                                        "type": "origins:high_humidity"
                                    }
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:change_resource",
                            "resource": "*:*_weather",
                            "change": 1,
                            "operation": "set"
                        }
                    },
                    {
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "inverted": true,
                                    "type": "origins:predicate",
                                    "predicate": "o:weather/is_rain"
                                },
                                {
                                    "inverted": true,
                                    "type": "origins:predicate",
                                    "predicate": "o:weather/is_dry_thunder"
                                },
                                {
                                    "inverted": true,
                                    "type": "origins:predicate",
                                    "predicate": "o:weather/is_thunderstorm"
                                }
                            ]
                        },
                        "action": {
                            "type": "origins:change_resource",
                            "resource": "*:*_weather",
                            "change": 0,
                            "operation": "set"
                        }
                    }
                ]
            }
        }
    }
}