                            {
                                "type": "origins:resource",
                                "resource": "*:*_level",
                                "comparison": "==",
                                "compare_to": 0
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_cd_timer",
                                "comparison": "==",
                                "compare_to": 41
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:change_resource",
                        "resource": "*:*_cd_timer",
                        "change": 0,
                        "operation": "set"
