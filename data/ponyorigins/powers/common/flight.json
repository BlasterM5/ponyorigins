{
    "name": "Flight",
    "description": "Jump while in the air to start flying. Be careful, hazard and injuries can cause temporary loss of flight.\n\nPress to manually disable/enable in order to fly with equipment.",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Jump while in the air to fly. Press [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to toggle"
            ],
            "sprite": "textures/item/feather.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Water disables flight",
            "sprite": "textures/particle/splash_0.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Spiderwebs disable flight",
            "sprite": "textures/block/cobweb.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Oil disables flight",
            "sprite": "textures/particle/splash_0.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Burned wings disable flight",
            "sprite": "textures/particle/flame.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Frozen wings disable flight",
            "sprite": "textures/block/ice.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Severe injuries (below 4 health) disable flight",
            "sprite": "textures/particle/damage.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Requires a half block clearance on either side and above to spread your wings",
            "sprite": "textures/item/feather.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Fall slowly and land gracefully unless exhausted",
            "sprite": "textures/item/feather.png"
        }
    ],
    "type": "origins:multiple",
    "block_wing_hit": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "block_wingstrike_check": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:creative_flying"
                },
                {
                    "inverted": true,
                    "type": "origins:gamemode",
                    "gamemode": "creative"
                },
                {
                    "type": "origins:or",
                    "conditions": [
                        {
                            "type": "origins:block_collision",
                            "offset_x": 0.5,
                            "offset_y": 0.5,
                            "offset_z": 0.5
                        },
                        {
                            "type": "origins:block_collision",
                            "offset_x": -0.5,
                            "offset_y": 0.5,
                            "offset_z": -0.5
                        },
                        {
                            "type": "origins:block_collision",
                            "offset_x": -0.5,
                            "offset_y": 0.5,
                            "offset_z": 0.5
                        },
                        {
                            "type": "origins:block_collision",
                            "offset_x": 0.5,
                            "offset_y": 0.5,
                            "offset_z": -0.5
                        }
                    ]
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_block_wing_hit",
                    "change": 1,
                    "operation": "set"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:or",
                                "conditions": [
                                    {
                                        "type": "origins:creative_flying"
                                    },
                                    {
                                        "type": "origins:and",
                                        "conditions": [
                                            {
                                                "type": "origins:fall_flying"
                                            },
                                            {
                                                "type": "origins:equipped_item",
                                                "equipment_slot": "chest",
                                                "item_condition": {
                                                    "inverted": true,
                                                    "type": "origins:ingredient",
                                                    "ingredient": {
                                                        "item": "elytra"
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "inverted": true,
                                "type": "origins:gamemode",
                                "gamemode": "creative"
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:play_sound",
                        "sound": "entity.player.big_fall",
                        "volume": 1,
                        "pitch": 1
                    }
                }
            ]
        },
        "falling_action": {
            "type": "origins:change_resource",
            "resource": "*:*_block_wing_hit",
            "change": 0,
            "operation": "set"
        }
    },
    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 40,
        "start_value": 0
    },
    "exhaust": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_active"
        },
        "type": "origins:action_over_time",
        "interval": 3,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:creative_flying"
            },
            "if_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:sprinting"
                },
                "if_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -2,
                    "operation": "add"
                },
                "else_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -1,
                    "operation": "add"
                }
            }
        }
    },
    "slow_falling_speed": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_active"
                },
                {
                    "type": "origins:fall_flying"
                },
                {
                    "inverted": true,
                    "type": "origins:sneaking"
                },
                {
                    "inverted": true,
                    "type": "origins:elytra_flight_possible",
                    "check_state": true,
                    "check_abilities": false
                }
            ]
        },
        "type": "origins:modify_falling",
        "velocity": 0.01,
        "take_fall_damage": true
    },
    "trigger": {
        "type": "origins:toggle",
        "active_by_default": true,
        "key": {
            "key": "key.origins.primary_active"
        },
        "retain_state": true
    },
    "creative_flight": {
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_active"
        },
        "type": "origins:creative_flight"
    },
    "tick": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_buffer",
                        "comparison": "<=",
                        "compare_to": 20
                    },
                    "if_action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:power",
                            "power": "ponyorigins:common/energy"
                        },
                        "if_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:if_else",
                                    "condition": {
                                        "type": "origins:resource",
                                        "resource": "ponyorigins:common/energy_level",
                                        "comparison": ">",
                                        "compare_to": 0
                                    },
                                    "if_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "ponyorigins:common/energy_level",
                                                "change": -1,
                                                "operation": "add"
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "*:*_buffer",
                                                "change": 20,
                                                "operation": "add"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        "else_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:if_else",
                                    "condition": {
                                        "type": "origins:food_level",
                                        "comparison": ">",
                                        "compare_to": 4
                                    },
                                    "if_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:exhaust",
                                                "amount": 0.1
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "*:*_buffer",
                                                "change": 20,
                                                "operation": "add"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:power_active",
                                "power": "*:*_active"
                            },
                            {
                                "type": "origins:fall_flying"
                            },
                            {
                                "inverted": true,
                                "type": "origins:sneaking"
                            },
                            {
                                "inverted": true,
                                "type": "origins:elytra_flight_possible",
                                "check_state": true,
                                "check_abilities": false
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:set_fall_distance",
                        "fall_distance": 0
                    }
                }
            ]
        }
    },
