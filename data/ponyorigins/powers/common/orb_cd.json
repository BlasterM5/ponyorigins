                            {
                                "type": "origins:execute_command",
                                "command": "advancement revoke @s only ponyorigins:changeling/love"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "scoreboard players set @s po.chtaint 100"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "scoreboard players set @s po.chlove 0"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "scoreboard players set @s po.chdorb 0"
                            }
                        ]
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "inverted": true,
                                "type": "origins:origin",
                                "origin": "ponyorigins:races/chaos"
                            },
                            {
                                "type": "origins:or",
