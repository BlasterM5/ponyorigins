        "execute_chosen_when_orb": true
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 60,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
            "bar_index": 1,
            "condition": {
                "type": "origins:resource",
                "resource": "*:*_cd",
                "comparison": "<",
                "compare_to": 60
            }
        }
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": "<",
            "compare_to": 60
        },
        "type": "origins:action_over_time",
        "interval": 1200,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": 1,
            "operation": "add"
        }
    },
    "orb_on_cd": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": "<",
            "compare_to": 60
        },
        "type": "origins:prevent_item_use",
        "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
                "item": "origins:orb_of_origin"
            }
        }
    },
    "orb_used": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": "==",
            "compare_to": 60
        },
        "type": "origins:action_on_item_use",
