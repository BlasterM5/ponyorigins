                                    {
                                        "type": "origins:and",
                                        "conditions": [
                                            {
                                                "type": "origins:origin",
                                                "origin": "ponyorigins:races/alicorn"
                                            },
                                            {
                                                "type": "origins:scoreboard",
                                                "objective": "po.chaos",
                                                "comparison": "==",
                                                "compare_to": 2
                                            }
                                        ]
                                    },
                                    {
                                        "inverted": true,
                                        "type": "origins:origin",
                                        "origin": "ponyorigins:races/alicorn"
                                    }
                                ]
                            },
                            {
                                "type": "origins:power",
                                "power": "ponyorigins:chaos/chaos",
                                "source": "ponyorigins:chaos/chaos"
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:revoke_power",
                        "power": "ponyorigins:chaos/chaos",
                        "source": "ponyorigins:chaos/chaos"
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:advancement",
                                "advancement": "ponyorigins:special/harmony_pact"
