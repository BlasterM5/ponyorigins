{
    "name": "Graze",
    "description": "Eat plants directly, as nature intended.",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Hold [",
                {
                    "keybind": "key.use"
                },
                "] to eat"
            ],
            "sprite": "minecraft:textures/item/wheat.png"
        }
    ],
    "type": "origins:multiple",
    "slow_movement": {
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_antilag",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:modify_attribute",
        "attribute": "minecraft:generic.movement_speed",
        "modifier": {
            "operation": "multiply_base_additive",
            "value": -0.75
        }
    },
    "no_sprint_eating": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_antilag",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_edible"
                }
            ]
        },
        "type": "origins:prevent_sprinting"
    },
    "antilag": {
        "type": "origins:resource",
        "min": 0,
        "max": 5,
        "start_value": 0
    },
    "eat_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 30,
        "start_value": 0,
        "min_action": {
            "type": "origins:change_resource",
            "resource": "*:*_sound_timer",
            "change": 0,
            "operation": "set"
        }
    },
    "sound_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 2,
        "start_value": 0,
        "max_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:power",
                        "power": "ponyorigins:common/diets/herbivore"
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "execute anchored eyes positioned ^ ^-0.5 ^0.45 run particle item minecraft:wheat ~ ~ ~ 0.1 0.1 0.1 0.055 4"
                    },
                    "else_action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:power",
                            "power": "ponyorigins:common/diets/pescavore"
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "execute anchored eyes positioned ^ ^-0.5 ^0.45 run particle item minecraft:kelp ~ ~ ~ 0.1 0.1 0.1 0.055 4"
                        },
                        "else_action": {
                            "type": "origins:if_else",
                            "condition": {
                                "type": "origins:power",
                                "power": "ponyorigins:common/lithovore"
                            },
                            "if_action": {
                                "type": "origins:execute_command",
                                "command": "execute anchored eyes positioned ^ ^-0.5 ^0.45 run particle item minecraft:diamond_block ~ ~ ~ 0.1 0.1 0.1 0.055 4"
                            },
                            "else_action": {
                                "type": "origins:execute_command",
                                "command": "execute anchored eyes positioned ^ ^-0.5 ^0.45 run particle item minecraft:wheat ~ ~ ~ 0.1 0.1 0.1 0.055 4"
                            }
                        }
                    }
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:entity.generic.eat",
                    "volume": 1,
                    "pitch": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_sound_timer",
                    "change": 0,
                    "operation": "set"
                }
            ]
        }
    },
    "cd": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "cd_timer": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_cd",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_cd",
            "change": -1,
            "operation": "add"
        }
    },
    "eat_cancel_decay": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_antilag",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_eat_timer",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 2,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_eat_timer",
            "change": -1,
            "operation": "add"
        }
    },
    "eating_tick": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_eat_timer",
                    "comparison": "<",
                    "compare_to": 30
                },
                {
                    "type": "origins:food_level",
                    "comparison": "<",
                    "compare_to": 20
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:equipped_item",
                            "equipment_slot": "mainhand",
                            "item_condition": {
                                "type": "origins:empty"
                            }
                        },
                        {
                            "type": "origins:equipped_item",
                            "equipment_slot": "offhand",
                            "item_condition": {
                                "type": "origins:empty"
                            }
                        }
                    ]
                }
            ]
        },
        "type": "origins:action_on_block_use",
        "block_condition": {
            "type": "origins:in_tag",
            "tag": "ponyorigins:plants"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:raycast",
                    "distance": 1,
                    "block": true,
                    "entity": false,
                    "shape_type": "outline",
                    "fluid_handling": "any",
                    "hit_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_eat_timer",
                                "change": 1,
                                "operation": "add"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_sound_timer",
                                "change": 1,
                                "operation": "add"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "*:*_antilag",
                                "change": 5,
                                "operation": "set"
                            }
                        ]
                    }
                }
            ]
        },
        "action_result": "consume_partial"
    },
    "antilag_decay": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_antilag",
            "comparison": ">",
            "compare_to": 0
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_antilag",
            "change": -1,
            "operation": "add"
        }
    },
    "eating_complete_plants": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_cd",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_eat_timer",
                    "comparison": "==",
                    "compare_to": 30
                },
                {
                    "type": "origins:food_level",
                    "comparison": "<",
                    "compare_to": 20
                },
                {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:equipped_item",
                            "equipment_slot": "mainhand",
                            "item_condition": {
                                "type": "origins:empty"
                            }
                        },
                        {
                            "type": "origins:equipped_item",
                            "equipment_slot": "offhand",
                            "item_condition": {
                                "type": "origins:empty"
                            }
                        }
                    ]
                }
            ]
        },
        "type": "origins:action_on_block_use",
        "block_condition": {
            "type": "origins:in_tag",
            "tag": "ponyorigins:plants"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_eat_timer",
                    "change": 0,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_sound_timer",
                    "change": 0,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_cd",
                    "change": 1,
                    "operation": "set"
                },
                {
                    "type": "origins:feed",
                    "food": 1,
                    "saturation": 0.5
                }
            ]
        },
        "block_action": {
            "type": "apugli:destroy",
            "drop_block": false
        },
        "action_result": "consume"
    }
}