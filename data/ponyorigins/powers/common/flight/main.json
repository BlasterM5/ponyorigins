{
    "name": "Flight",
    "description": "Jump in the air to spread your wings and take flight! Be careful not to damage them or get them caught in things, or you may be grounded for a while.\n\nManually disable/enable with your primary power to fly with equipment.",
    "badges": [
        {
            "type": "origins:tooltip",
            "text": [
                "Jump while in the air to fly. Press [",
                {
                    "keybind": "key.origins.primary_active"
                },
                "] to toggle"
            ],
            "sprite": "minecraft:textures/item/feather.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Water disables flight",
            "sprite": "minecraft:textures/particle/splash_0.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Spiderwebs disable flight",
            "sprite": "minecraft:textures/block/cobweb.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Oil disables flight",
            "sprite": "minecraft:textures/particle/splash_0.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Burned wings disable flight",
            "sprite": "minecraft:textures/particle/flame.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Severe injuries (below 4 health) disable flight",
            "sprite": "minecraft:textures/particle/damage.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Requires a half block clearance on either side and above to spread your wings",
            "sprite": "minecraft:textures/item/feather.png"
        },
        {
            "type": "origins:tooltip",
            "text": "Fall slowly and land gracefully unless exhausted",
            "sprite": "minecraft:textures/item/feather.png"
        }
    ],
    "type": "origins:multiple",
    "block_wing_hit": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "block_wing_hit_detection": {
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:creative_flying"
                },
                {
                    "inverted": true,
                    "type": "origins:gamemode",
                    "gamemode": "creative"
                },
                {
                    "type": "origins:or",
                    "conditions": [
                        {
                            "type": "origins:block_collision",
                            "offset_x": 0.5,
                            "offset_y": 0,
                            "offset_z": 0
                        },
                        {
                            "type": "origins:block_collision",
                            "offset_x": -0.5,
                            "offset_y": 0,
                            "offset_z": 0
                        },
                        {
                            "type": "origins:block_collision",
                            "offset_x": 0,
                            "offset_y": 0.5,
                            "offset_z": 0
                        }
                    ]
                }
            ]
        },
        "type": "origins:action_over_time",
        "interval": 20,
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_block_wing_hit",
                    "change": 1,
                    "operation": "set"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:or",
                                "conditions": [
                                    {
                                        "type": "origins:creative_flying"
                                    },
                                    {
                                        "type": "origins:and",
                                        "conditions": [
                                            {
                                                "type": "origins:fall_flying"
                                            },
                                            {
                                                "type": "origins:equipped_item",
                                                "equipment_slot": "chest",
                                                "item_condition": {
                                                    "inverted": true,
                                                    "type": "origins:ingredient",
                                                    "ingredient": {
                                                        "item": "minecraft:elytra"
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "inverted": true,
                                "type": "origins:gamemode",
                                "gamemode": "creative"
                            }
                        ]
                    },
                    "if_action": {
                        "type": "origins:play_sound",
                        "sound": "entity.player.big_fall",
                        "volume": 1,
                        "pitch": 1
                    }
                }
            ]
        },
        "falling_action": {
            "type": "origins:change_resource",
            "resource": "*:*_block_wing_hit",
            "change": 0,
            "operation": "set"
        }
    },
    "buffer": {
        "type": "origins:resource",
        "min": 0,
        "max": 40,
        "start_value": 0
    },
    "recharge": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_buffer",
            "comparison": "<=",
            "compare_to": 20
        },
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:power",
                "power": "ponyorigins:common/energy"
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "ponyorigins:common/energy_level",
                        "change": -1,
                        "operation": "add"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 20,
                        "operation": "add"
                    }
                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:exhaust",
                        "amount": 0.1
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_buffer",
                        "change": 20,
                        "operation": "add"
                    }
                ]
            }
        }
    },
    "exhaust": {
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_active",
            "comparison": "==",
            "compare_to": 1
        },
        "type": "origins:action_over_time",
        "interval": 3,
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:creative_flying"
            },
            "if_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:sprinting"
                },
                "if_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -2,
                    "operation": "add"
                },
                "else_action": {
                    "type": "origins:change_resource",
                    "resource": "*:*_buffer",
                    "change": -1,
                    "operation": "add"
                }
            }
        }
    },
    "slower_in_water": {
        "condition": {
            "type": "origins:and",
